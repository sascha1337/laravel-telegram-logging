{
  "name": "rafaellaurindo/laravel-telegram-logging",
  "description": "Send Laravel logs to a Telegram chat via Telegram Bot.",
  "homepage": "https://github.com/rafaellaurindo/laravel-telegram-logging",
  "require": {
    "php": "^7.4",
    "laravel/framework": "^6 || ^7 || ^8",
    "monolog/monolog": "^2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Rafael Laurindo",
      "email": "rafaelfilholm@gmail.com",
      "homepage": "https://rafaellaurindo.com.br"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "RLaurindo\\TelegramLogger\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RLaurindo\\TelegramLogger\\TelegramLoggerServiceProvider"
      ]
    }
  }
}
